{
    "name": "rescript-dnd",
    "sources": [
        {
            "dir": "src",
            "subdirs": true
        },
        {
            "dir": "examples",
            "type": "dev",
            "subdirs": true
        }
    ],
    "bs-dependencies": [
        "@rescript/core",
        "@rescript/react",
        "rescript-webapi",
        "rescript-logger"
    ],
    "bs-dev-dependencies": [
        "rescript-classnames"
    ],
    "jsx": {
        "version": 4
    },
    "bsc-flags": [
        "-open Belt"
    ],
    "ppx-flags": [
        [
            "rescript-logger/ppx",
            "--lib=rescript-dnd"
        ]
    ],
    "package-specs": {
        "module": "esmodule",
        "in-source": true
    },
    "suffix": ".res.js",
    "warnings": {
        "number": "+A-40-42-44"
    }
}
