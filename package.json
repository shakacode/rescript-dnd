{
  "name": "re-dnd",
  "version": "1.1.0",
  "description": "Reasonable drag-n-drop for reason-react",
  "main": "src/Dnd.re",
  "author": "Alex Fedoseev <alex.fedoseev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel examples/index.html",
    "prestart": "yarn run build:bs:dev",
    "build": "parcel build examples/index.html",
    "prebuild": "yarn run build:bs",
    "build:bs": "BS_LOG=off bsb -clean-world -make-world",
    "build:bs:dev": "BS_LOG=*,re-dnd=* bsb -clean-world -make-world",
    "watch:bs": "BS_LOG=*,re-dnd=* bsb -clean-world -make-world -w",
    "clean": "yarn run clean:bs && yarn run clean:dist",
    "clean:bs": "bsb -clean-world",
    "clean:dist": "rm -rf dist",
    "test": "exit 0",
    "refmt": "find -E . -regex '.+\\.rei?' -exec refmt --in-place {} \\;",
    "deploy": "now deploy dist --public --name re-dnd",
    "predeploy": "yarn run build"
  },
  "files": [
    "src",
    "bsconfig.json"
  ],
  "keywords": [
    "react",
    "reason",
    "reason-react",
    "reasonml",
    "ocaml",
    "bucklescript",
    "ui",
    "dnd",
    "drag-n-drop"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexfedoseev/re-dnd.git"
  },
  "dependencies": {
    "bs-log": "1.1.0",
    "bs-webapi": "0.15.6"
  },
  "devDependencies": {
    "autoprefixer": "9.7.3",
    "bs-platform": "7.0.1",
    "bsb-js": "1.1.7",
    "parcel-bundler": "1.12.4",
    "re-classnames": "4.0.0",
    "reason-react": "0.7.0"
  }
}
