{
  "name": "rescript-dnd",
  "version": "8.0.1",
  "description": "Drag-n-drop for @rescript/react",
  "author": "Alex Fedoseev <alex@35mil.me>",
  "license": "MIT",
  "scripts": {
    "start": "parcel examples/index.html",
    "prestart": "yarn run res:build:dev",
    "build": "parcel build examples/index.html",
    "prebuild": "yarn run res:build",
    "res:build": "RES_LOG=off rescript build",
    "res:build:dev": "RES_LOG=*,rescript-dnd=* rescript build",
    "res:watch": "RES_LOG=*,rescript-dnd=* rescript build -w",
    "res:clean": "rescript clean",
    "clean": "rm -rf dist && yarn run res:clean",
    "test": "exit 0",
    "format": "rescript format",
    "deploy": "vercel deploy dist --prod",
    "predeploy": "yarn run build"
  },
  "files": [
    "src",
    "rescript.json"
  ],
  "keywords": [
    "react",
    "rescript",
    "rescript-react",
    "reason",
    "reason-react",
    "reasonml",
    "ocaml",
    "bucklescript",
    "ui",
    "dnd",
    "drag-n-drop"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shakacode/rescript-dnd"
  },
  "dependencies": {
    "rescript-logger": "4.0.0"
  },
  "peerDependencies": {
    "@rescript/react": ">=0.14.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "rescript": ">=12.0.0",
    "rescript-webapi": ">=0.6.0"
  },
  "devDependencies": {
    "@rescript/react": "0.14.0",
    "autoprefixer": "10.4.23",
    "parcel": "2.16.3",
    "postcss": "8.5.6",
    "process": "^0.11.10",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rescript": "12.0.2",
    "rescript-classnames": "8.0.0",
    "rescript-webapi": "0.10.0"
  },
  "packageManager": "yarn@1.22.15+sha512.033a040f18f6e7a04e4bf8ea0d7037a63ca19a2e054410543206284c7238108b7612144492f1f4b283d512d9c3f8340825f53947d6ca85c6751fd97cccf59ea0"
}
